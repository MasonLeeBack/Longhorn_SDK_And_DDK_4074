Test Assertion_A134317

Var p:Property, root:Item
Var dt
{
    PrintDescriptionFromXML('13.4.3.17')
    StartWia()
    GetRoot(root)
    GetDeviceType(dt)

    If (dt != StiDeviceTypeScanner)
    Then Exit Passed

    If (!GetProperty(root, p, WIA_DPS_ENDORSER_STRING))
    Then Exit Passed

    If (!CheckPropertyAccessFromXML(p) )
    Then
    {
        Writeln '\tIncorrect access'
        Exit Failed
    }

    If (!CheckPropertyTypeFromXML(p) )
    Then
    {
        Writeln '\tIncorrect vartype'
        Exit Failed
    }

    If( IsPropertyReadWrite(p) )
    Then
    {
        If (!SetPropertyStr(root, WIA_DPS_ENDORSER_STRING,'TEST STRING'))
        Then
        {
            Writeln '\tSetting property value failed'
            Exit Failed
        }
    }
    Else
    {
       If (SetPropertyValidValue(root,p) Or SetPropertyInvalidValue(root,p) )
       Then 
       {
            Writeln '\tSetting a read-only property value succeeded'
            Exit Failed
       }
    }

    StopWia()
}
