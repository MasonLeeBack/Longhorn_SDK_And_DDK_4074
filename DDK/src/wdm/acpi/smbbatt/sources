TARGETNAME=smbbatt
TARGETPATH=obj
TARGETTYPE=DRIVER

MSC_COMPILER_WARNINGS=-W3 -WX

USE_MAPSYM=1

SOURCES=smbbatt.c       \
        smbmisc.c       \
        pnppower.c      \
        smbwmi.c        \
        smbbatt.rc

INCLUDES=.;

TARGETLIBS=$(DDK_LIB_PATH)\battc.lib
